{
  "manifest_version": 2,

  "name": "RoughVideoDownloader",
  "version": "0.1",
  "background": {"scripts": ["background.js"]},

  "permissions": [
    "contextMenus", "nativeMessaging"
  ],
  
  "content_scripts": [{
    "matches": ["http://www.flvcd.com/parse.php?kw=*"],
    "js": ["flvcdParse.js"]
  }, {
	"matches": ["http://www.flvcd.com/url.php"],
	"js": ["lib/jquery-2.1.3.js","flvcdURLs.js"]
  }]
}